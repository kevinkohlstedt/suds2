<div class="page-header">
  <h1>
    Brewery to beers
    <a href="/brewery_to_beers/new" class="btn btn-lg btn-success">New Brewery to beer</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Beer</th>
        <th>Brewery</th>
        <th>Actions</th>
      </tr>

      <% @brewery_to_beers.each do |brewery_to_beer| %>
      <tr>
        <td><%= brewery_to_beer.beer_id %></td>
        <td><%= brewery_to_beer.brewery_id %></td>
        <td>
          <a href="/brewery_to_beers/<%= brewery_to_beer.id %>" class="btn btn-primary">Show</a>
          <a href="/brewery_to_beers/<%= brewery_to_beer.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_brewery_to_beer/<%= brewery_to_beer.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
